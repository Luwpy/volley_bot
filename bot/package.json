{
  "name": "bot",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "check": "tsc --noEmit && echo âœ” Ok",
    "build": "tsc",
    "dev": "bun --env-file .env ./src/index.ts",
    "dev:dev": "bun --env-file .env.dev ./src/index.ts",
    "watch": "bun --watch --env-file .env ./src/index.ts",
    "watch:dev": "bun --watch --env-file .env.dev ./src/index.ts",
    "start": "bun --env-file .env .",
    "start:dev": "bun --env-file .env.dev .",
    "prestart": "bunx prisma migrate deploy",
    "postinstall": "bunx prisma generate"
  },
  "dependencies": {
    "@fastify/cors": "10.0.1",
    "@magicyan/discord": "1.5.1",
    "@prisma/client": "^6.13.0",
    "@reliverse/reglob": "1.0.0",
    "chalk": "5.4.1",
    "discord.js": "14.21.0",
    "fastify": "5.0.0",
    "http-status-codes": "2.3.0",
    "rou3": "0.7.3",
    "zod": "4.0.14"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "typescript": "5.7.2"
  },
  "imports": {
    "#env": [
      "./build/env.js"
    ],
    "#base": [
      "./build/discord/base/index.js"
    ],
    "#functions": [
      "./build/functions/index.js"
    ],
    "#database": [
      "./build/database/index.js"
    ],
    "#server": [
      "./build/server/index.js"
    ],
    "#menus": [
      "./build/menus/index.js"
    ],
    "#tools": [
      "./build/tools/index.js"
    ],
    "#lib/*": [
      "./build/lib/*"
    ]
  },
  "baseVersion": "1.4.0"
}
