model Guild {
  id         String         @id
  createdAt  DateTime       @default(now())
  updatedAt  DateTime       @updatedAt
  members    Member[]
  channels   GuildChannel[]
  teams      Team[]

  @@map("guilds")
}

model GuildChannel {
  id      String @id 
  name    String
  url     String
  guildId String

  guild Guild @relation(fields: [guildId], references: [id])

  @@map("guild_channels")
}
