model Member {
  id         String   @unique
  guildId    String
  guild      Guild    @relation(fields: [guildId], references: [id])
  characters Character[]
  ownedTeams Team[]   @relation("TeamOwner")
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@id([id, guildId])
  @@map("members")
}
